<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<l:Grid id="DSTGRID" defaultSpan="L12 M12 S12" width="auto" vSpacing="0">
		<l:content>
			<IconTabBar expanded="{device>/isNoPhone}">
				<items>
					<IconTabFilter icon="sap-icon://hint" tooltip="Sponsorship Transfer - Staff / Student">
						<VBox id="DSTReject">
							<Text text="Rejection Comments" class="clDPSPTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Text id="iddstfincomments"/>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
						<VBox id="DSTCIDetails">
							<Text text="Candidate Information" width="400px" class="clDPSPTransfer titlrStyle"/>
							<!--  >l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content -->
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="English First Name:" design="Bold" width="275px"/>
											<Text id="dst_firstname" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="English Middle Name:" design="Bold" width="275px"/>
											<Text id="dst_middlename" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="English Last Name:" design="Bold" width="275px"/>
											<Text id="dst_lastname" width="275px"/>
										</VBox>
									</HBox>
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Arabic First Name:" design="Bold" width="275px"/>
											<Text id="dst_arfirstname" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Arabic Middle Name:" design="Bold" width="275px"/>
											<Text id="dst_armiddlename" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Arabic Last Name:" design="Bold" width="275px"/>
											<Text id="dst_arlastname" width="275px"/>
										</VBox>
									</HBox>
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Iqama Number:" design="Bold" width="275px"/>
											<Text id="dst_iqama" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Cost Center Number:" design="Bold" width="275px"/>
											<Text id="dst_costcenter" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Date of Birth:" design="Bold" width="275px"/>
											<Text id="dst_birthdate" width="275px"/>
										</VBox>
									</HBox>
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Nationality:" design="Bold" width="275px"/>
											<Text id="dst_nation" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Religion:" design="Bold" width="275px"/>
											<Text id="dst_religion" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Expeditor:" design="Bold" width="275px"/>
											<Text id="idDSTExpeditor" width="275px"/>
										</VBox>
									</HBox>
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Current Sponsor Iqama Number:" design="Bold" width="275px"/>
											<Text id="dst_currsponsornum" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Current Sponsor Name:" design="Bold" width="275px"/>
											<Text id="dst_currsponsorname" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Current Sponsor Arabic Name:" design="Bold" width="275px"/>
											<Text id="dst_arcurrsponsorname" width="275px"/>
										</VBox>
									</HBox>
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="New Sponsor Iqama Number:" design="Bold" width="275px"/>
											<Text id="dst_newsponsornum" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="New Sponsor Name:" design="Bold" width="275px"/>
											<Text id="dst_newsponsorname" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="New Sponsor Arabic Name:" design="Bold" width="275px"/>
											<Text id="dst_arnewsponsorname" width="275px"/>
										</VBox>
									</HBox>
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Current Job Title:" design="Bold" width="275px"/>
											<Text id="dst_currjobtitle" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Current Job Title Arabic:" design="Bold" width="275px"/>
											<Text id="dst_arcurrjobtitle" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Label text="Change Job Title:" design="Bold" width="275px"/>
											<Text id="dst_jobchange" width="275px"/>
										</VBox>
									</HBox>
									<HBox id="dst_vbnewjob" class="destilPartHeaderKeyStyle">
										<VBox id="dst_vbennewjob" class="destilPartHeaderDSTStyle">
											<Label text="New Job Title:" design="Bold" width="275px"/>
											<Text id="dst_newjobtitle" width="275px"/>
										</VBox>
										<VBox id="dst_vbarnewjob" class="destilPartHeaderDSTStyle">
											<Label text="New Job Title Arabic:" design="Bold" width="275px"/>
											<Text id="dst_arnewjobtitle" width="275px"/>
										</VBox>
									</HBox>
								<!-- /l:content>
							</l:Grid -->
						</VBox>
						<VBox id="DSTAttachments">
							<Text text="Attachments"  width="400px" class="clDPSPTransfer titlrStyle"/>
							<!-- l:Grid defaultSpan="L3 M3 S3" width="auto" vSpacing="0">
								<l:content -->
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Link id="idDSTatt1" emphasized="true" target="_blank" href="#" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Link id="idDSTatt2" emphasized="true" target="_blank" href="#" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Link id="idDSTatt3" emphasized="true" target="_blank" href="#" width="275px"/>
										</VBox>
									</HBox>
									<HBox class="destilPartHeaderKeyStyle">
										<VBox class="destilPartHeaderDSTStyle">
											<Link id="idDSTatt4" emphasized="true" target="_blank" href="#" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Link id="idDSTatt5" emphasized="true" target="_blank" href="#" width="275px"/>
										</VBox>
										<VBox class="destilPartHeaderDSTStyle">
											<Link id="idDSTatt6" emphasized="true" target="_blank" href="#" width="275px"/>
										</VBox>
									</HBox>
								<!-- /l:content>
							</l:Grid -->
						</VBox>
						<VBox id="DSTPrefHead">
							<Text text="Manage your Preference" class="clDPSPTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Collection" class="keyClass"/>
						                    <RadioButtonGroup id="idDSTCollection" selectedIndex="0" columns="2" select="onRadioBtnSelect" enabled="false">
						                      <buttons>
						                        <RadioButton text="Self Drop off"/>
						                        <RadioButton text="UPS Pickup" />
						                      </buttons>
						                    </RadioButtonGroup>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Delivery" class="keyClass"/>
						                    <RadioButtonGroup id="idDSTDelivery" selectedIndex="0" columns="2" select="onRadioBtnSelect" enabled="false">
						                      <buttons>
						                        <RadioButton text="Self Pick Up"/>
						                        <RadioButton text="UPS Delivery" />
						                      </buttons>
						                    </RadioButtonGroup>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
						<VBox id="DSTPrefBody">
							<Text text="Preference Details" class="clDPSPTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Name:" class="keyClass"/>
											<Text id="dstcpdname" text="{preferenceModel>/first_name} {preferenceModel>/last_name}"/>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="KAUST ID:" class="keyClass"/>
											<Text id="dstcpdkaustid" text="{preferenceModel>/kaust_id}"/>
                                            
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Mobile:" class="keyClass"/>
											<Text id="dstcpdmobile" text="{preferenceModel>/mobile_no}"/>
										</HBox>
									</VBox>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Building Name / Number:" class="keyClass"/>
											<Text id="dstcpdbldno" text="{preferenceModel>/building_name}"/>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Level:" class="keyClass"/>
											<Text id="dstcpdlevel" text="{preferenceModel>/level_b}"/>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Room No / Cubicle:" class="keyClass"/>
											<Text id="dstcpdbldname" text="{preferenceModel>/building_no}"/>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
						<VBox id="DSTTracking">
							<Text text="Tracking Details" class="clDPSPTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Tracking Number:" class="keyClass"/>
											<Text id="idDSTTrackingNum"/>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
					</IconTabFilter>

					 <IconTabFilter icon="sap-icon://comment">
						<List id="commentHistory" headerText="" noDataText="No Comments Log" growing="true" growingThreshold="4" growingScrollToLoad="false"
							items="{commentData>/CommentsInfo}">
							<FeedListItem sender="{commentData>t_name}" text="{commentData>comments}" showIcon="false"
								timestamp="{ path: 'commentData>timestamp', formatter: '.formatter.commentDate1' }"/>
						</List>
					</IconTabFilter>
					 
					<IconTabFilter icon="sap-icon://customer-history" tooltip="Status">
						<Table id="TblHistory" items="{historyModel>/}" >
						<columns>
							<Column width="12em">
								<Text text="User Name" />
							</Column>
							<Column minScreenWidth="Tablet" demandPopin="true">
								<Text text="Status"/>
							</Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
								<Text text="Date" />
							</Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
								<Text text="Time" />
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
                                    <Text text="{historyModel>Fname}"/>
										<Text text="{path:'historyModel>statusdesc'}" />
										<Text text="{path:'historyModel>timestamp', formatter:'.formatter.convertDate'}" />
										<Text text="{path:'historyModel>timestamp', formatter:'.formatter.convertTime'}" />
								</cells>
							</ColumnListItem>
						</items>
						</Table> 
					</IconTabFilter>
				</items>
			</IconTabBar>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>