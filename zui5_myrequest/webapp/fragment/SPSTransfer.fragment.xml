<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<l:Grid id="SSTGRID" defaultSpan="L12 M12 S12" width="auto" vSpacing="0">
		<l:content>
			<IconTabBar expanded="{device>/isNoPhone}">
				<items>
					<IconTabFilter icon="sap-icon://hint" tooltip="Sponsorship Transfer (Spouse)">
						<VBox id="SSTReject">
							<Text text="Rejection Comments" class="clSPSTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Text id="idsstfincomments"/>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
						<VBox id="SSTSTDetails">
							<Text text="Sponsorship Transfer" width="400px" class="clSPSTransfer titlrStyle"/>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle1">
									<Label text="Is current sponsor in KAUST?" design="Bold"/>
									<Text id="sst_sponsortype" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle" visible="false">
								<VBox class="destilPartHeaderSSTStyle1">
									<Label text="Is Marriage Certificate issued from KSA?" design="Bold"/>
									<Text id="sst_marcertificate" width="275px"/>
								</VBox>	
							</HBox>
						</VBox>
						<VBox id="SSTMCDetails" visible="false">
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle1">
									<Label text="Is Marriage Certificate attested from saudi embassy in your home country?" design="Bold"/>
									<Text id="sst_certattested" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle1">
									<Label text="Is Marriage Certificate attested by MOFA?" design="Bold"/>
									<Text id="sst_certmofa" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle1">
									<Label text="Is Marriage Certificate translated to Arabic from an authorized office?" design="Bold"/>
									<Text id="sst_certtranslated" width="275px"/>
								</VBox>
							</HBox>
						</VBox>						
						<VBox id="SSTRIDetails">
							<Text text="Spouse Information" width="400px" class="clSPSTransfer titlrStyle"/>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Does the spouse have valid KAUST ID:" design="Bold" width="350px"/>
									<Text id="sst_kaustflg" width="275px"/>
								</VBox>
							</HBox>
							<HBox id="sps_kaustid" class="destilPartHeaderKeyStyle"  visible="false">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Spouse KAUST ID:" design="Bold" width="275px"/>
									<Text id="sst_kaustid" width="275px"/>
								</VBox>
							</HBox>							
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="English First Name:" design="Bold" width="275px"/>
									<Text id="sst_firstname" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="English Middle Name:" design="Bold" width="275px"/>
									<Text id="sst_middlename" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="English Last Name:" design="Bold" width="275px"/>
									<Text id="sst_lastname" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Arabic First Name:" design="Bold" width="275px"/>
									<Text id="sst_arfirstname" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Arabic Middle Name:" design="Bold" width="275px"/>
									<Text id="sst_armiddlename" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Arabic Last Name:" design="Bold" width="275px"/>
									<Text id="sst_arlastname" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Iqama Number:" design="Bold" width="275px"/>
									<Text id="sst_iqama" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Date of Birth:" design="Bold" width="275px"/>
									<Text id="sst_birthdate" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Nationality:" design="Bold" width="275px"/>
									<Text id="sst_nation" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Religion:" design="Bold" width="275px"/>
									<Text id="sst_religion" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Gender:" design="Bold" width="275px"/>
									<Text id="sst_gender" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Expeditor:" design="Bold" width="275px"/>
									<Text id="idSSTExpeditor" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Current Sponsor Number:" design="Bold" width="275px"/>
									<Text id="sst_currsponsornum" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Current Sponsor Name:" design="Bold" width="275px"/>
									<Text id="sst_currsponsorname" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Current Sponsor Arabic Name:" design="Bold" width="275px"/>
									<Text id="sst_arcurrsponsorname" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="New Sponsor Number:" design="Bold" width="275px"/>
									<Text id="sst_newsponsornum" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="New Sponsor Name:" design="Bold" width="275px"/>
									<Text id="sst_newsponsorname" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="New Sponsor Arabic Name:" design="Bold" width="275px"/>
									<Text id="sst_arnewsponsorname" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Job Title:" design="Bold" width="275px"/>
									<Text id="sst_jobtitle" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Transfer Type:" design="Bold" width="275px"/>
									<Text id="sst_transfertype" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Label text="Sponsorship Transfer Fee:" design="Bold" width="275px"/>
									<Text id="sst_transferfee" width="275px"/>
								</VBox>
							</HBox>
						</VBox>
						<VBox id="SSTAttachments">
							<Text text="Attachments"  width="400px" class="clSPSTransfer titlrStyle"/>
							<HBox class="destilPartHeaderKeyStyle">
								<VBox class="destilPartHeaderSSTStyle">
									<Link id="idSSTatt1" emphasized="true" target="_blank" href="#" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Link id="idSSTatt2" emphasized="true" target="_blank" href="#" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Link id="idSSTatt3" emphasized="true" target="_blank" href="#" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Link id="idSSTatt4" emphasized="true" target="_blank" href="#" width="275px"/>
								</VBox>
							</HBox>
							<HBox class="destilPartHeaderKeyStyle" visible="false">
								<VBox class="destilPartHeaderSSTStyle">
									<Link id="idSSTatt5" emphasized="true" target="_blank" href="#" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Link id="idSSTatt6" emphasized="true" target="_blank" href="#" width="275px"/>
								</VBox>
								<VBox class="destilPartHeaderSSTStyle">
									<Link id="idSSTatt7" emphasized="true" target="_blank" href="#" width="275px"/>
								</VBox>
							</HBox>
						</VBox>
						<VBox id="SSTPrefHead">
							<Text text="Manage your Preference" class="clSPSTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Collection" class="keyClass"/>
						                    <RadioButtonGroup id="idSSTCollection" selectedIndex="0" columns="2" select="onRadioBtnSelect" enabled="false">
						                      <buttons>
						                        <RadioButton text="Self Drop off"/>
						                        <RadioButton text="UPS Pickup" />
						                      </buttons>
						                    </RadioButtonGroup>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Delivery" class="keyClass"/>
						                    <RadioButtonGroup id="idSSTDelivery" selectedIndex="0" columns="2" select="onRadioBtnSelect" enabled="false">
						                      <buttons>
						                        <RadioButton text="Self Pick Up"/>
						                        <RadioButton text="UPS Delivery" />
						                      </buttons>
						                    </RadioButtonGroup>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
						<VBox id="SSTPrefBody">
							<Text text="Preference Details" class="clSPSTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Name:" class="keyClass"/>
											<Text id="sstcpdname" text="{preferenceModel>/first_name} {preferenceModel>/last_name}"/>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="KAUST ID:" class="keyClass"/>
											<Text id="sstcpdkaustid" text="{preferenceModel>/kaust_id}"/>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Mobile:" class="keyClass"/>
											<Text id="sstcpdmobile" text="{preferenceModel>/mobile_no}"/>
										</HBox>
									</VBox>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Building Name / Number:" class="keyClass"/>
											<Text id="sstcpdbldno" text="{preferenceModel>/building_name}"/>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Level:" class="keyClass"/>
											<Text id="sstcpdlevel" text="{preferenceModel>/level_b}"/>
										</HBox>
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Room No / Cubicle:" class="keyClass"/>
											<Text id="sstcpdbldname" text="{preferenceModel>/building_no}"/>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
						<VBox id="SSTTracking">
							<Text text="Tracking Details" class="clSPSTransfer titlrStyle"/>
							<l:Grid defaultSpan="L6 M6 S12" width="auto" vSpacing="0">
								<l:content>
									<VBox class="destilPartHeaderStyle">
										<HBox class="destilPartHeaderKeyStyle">
											<Label text="Tracking Number:" class="keyClass"/>
											<Text id="idSSTTrackingNum"/>
										</HBox>
									</VBox>
								</l:content>
							</l:Grid>
						</VBox>
					</IconTabFilter>

					<IconTabFilter icon="sap-icon://comment">
						<List id="commentHistory" headerText="" noDataText="No Comments Log" growing="true" growingThreshold="4" growingScrollToLoad="false"
							items="{commentData>/CommentsInfo}">
							<FeedListItem sender="{commentData>t_name}" text="{commentData>comments}" showIcon="false"
								timestamp="{ path: 'commentData>timestamp', formatter: '.formatter.commentDate1' }"/>
						</List>
					</IconTabFilter>
					 
					<IconTabFilter icon="sap-icon://customer-history" tooltip="Status">
						<Table id="TblHistory" items="{historyModel>/}" >
						<columns>
							<Column width="12em">
								<Text text="User Name" />
							</Column>
							<Column minScreenWidth="Tablet" demandPopin="true">
								<Text text="Status"/>
							</Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
								<Text text="Date" />
							</Column>
							<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
								<Text text="Time" />
							</Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{historyModel>Fname}" />
									<Text text="{path:'historyModel>statusdesc'}" />
										<Text text="{path:'historyModel>timestamp', formatter:'.formatter.convertDate'}" />
										<Text text="{path:'historyModel>timestamp', formatter:'.formatter.convertTime'}" />
								</cells>
							</ColumnListItem>
						</items>
						</Table> 
					</IconTabFilter>
				</items>
			</IconTabBar>
		</l:content>
	</l:Grid>
</core:FragmentDefinition>